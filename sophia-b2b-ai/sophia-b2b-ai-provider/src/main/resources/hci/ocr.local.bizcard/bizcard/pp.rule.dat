  <object id="0" name="UKN" class="0"><!--最后无法分类的-->
    <keywords type="singleKey">
      <word matchMethod="string" matchValueType="inlineAfter" rulePriority="4" matchLoc="start">:！</word><!--去除一下冒号-->
    </keywords>
  </object>

